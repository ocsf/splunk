{
  "caption": "Detection Report",
  "category": "findings",
  "description": "Detection events report the suspicious activities detected by rule-based or analytics engines.",
  "name": "detection_report",
  "uid": 1,
  "attributes": {
    "$include": [
      "includes/classification.json"
    ],
    "activity_id": {
      "enum": {
        "1": {
          "caption": "New",
          "description": "New suspicious activity."
        }
      }
    },
    "analytic_stories": {
      "group": "context"
    },
    "attacks": {
      "caption": "MITRE ATT&CKÂ®",
      "description": "The array of adversary tactics and techniques identified by the detections or analytics.",
      "group": "context"
    },
    "cis_csc": {
      "group": "context"
    },
    "confidence": {
      "group": "primary"
    },
    "confidence_id": {
      "group": "primary",
      "requirement": "recommended"
    },
    "count": {
      "group": "occurrence"
    },
    "cves": {
      "group": "context"
    },
    "devices": {
      "group": "primary"
    },
    "duration": {
      "description": "The duration of the detection time period in milliseconds.",
      "group": "occurrence"
    },
    "end_time": {
      "description": "The end time of the detection time period.",
      "group": "occurrence",
      "requirement": "recommended",
      "type": "timestamp_t"
    },
    "enrichments": {
      "group": "context"
    },
    "impact": {
      "group": "primary"
    },
    "impact_id": {
      "group": "primary",
      "requirement": "recommended"
    },
    "kill_chain": {
      "group": "context"
    },
    "message": {
      "description": "The description of the detection report.",
      "group": "context"
    },
    "metadata": {
      "group": "context",
      "requirement": "required"
    },
    "nist": {
      "group": "context"
    },
    "observables": {
      "description": "The observables associated with the detection report.",
      "group": "context"
    },
    "risk_level": {
      "group": "primary"
    },
    "risk_level_id": {
      "group": "primary",
      "requirement": "required"
    },
    "risk_score": {
      "group": "primary",
      "requirement": "required"
    },
    "rule": {
      "group": "primary",
      "requirement": "required"
    },
    "severity": {
      "group": "classification",
      "requirement": "optional"
    },
    "severity_id": {
      "group": "classification",
      "requirement": "required"
    },
    "start_time": {
      "description": "The start time of the detection time period.",
      "group": "occurrence",
      "requirement": "recommended",
      "type": "timestamp_t"
    },
    "supporting_evidence": {
      "group": "context"
    },
    "time": {
      "group": "occurrence",
      "requirement": "required"
    },
    "users": {
      "group": "primary"
    }
  }
}
